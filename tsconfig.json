{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@utils/*": ["./src/utils/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@assets/*": ["./src/assets/*"],
      "@storage/*": ["./src/storage/*"],
      "@screens/*": ["./src/screens/*"],
      "@context/*": ["./src/context/*"],
      "@firebaseApp/*": ["./src/firebaseApp/*"],
      "@components/*": ["./src/components/*"],
    }
  }
}
